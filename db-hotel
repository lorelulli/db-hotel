SELECT * FROM `ospiti` WHERE `document_type`= 'CI';
SELECT * FROM `ospiti` WHERE `date_of_birth` > '1988';
SELECT * FROM `ospiti` WHERE `date_of_birth` < '2000-05-12';
SELECT * FROM `ospiti` WHERE `name` LIKE 'D%';
SELECT SUM(price) FROM `pagamenti` WHERE `status` = 'accepted';
SELECT `price` FROM `pagamenti` WHERE `status` = 'accepted' GROUP BY `price` ORDER BY `pagamenti`.`price` DESC;
SELECT * FROM `ospiti` WHERE `document_type`= 'Driver License' AND `date_of_birth` > '1974-12-31' AND `date_of_birth` < '1976-01-01';
SELECT SUM(beds) FROM `stanze`;
